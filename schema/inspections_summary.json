{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The Root Schema",
    "description": "The root schema comprises the entire JSON document.",
    "required": [
        "data",
        "labels",
        "last_update"
    ],
    "properties": {
        "data": {
            "$id": "#/properties/data",
            "type": "object",
            "title": "The Data Schema",
            "description": "An explanation about the purpose of this instance.",
            "default": {},
            "examples": [
                {
                    "地方衛生研究所等": [
                        0.0,
                        0.0
                    ],
                    "民間検査機関等": [
                        0.0,
                        0.0
                    ]
                }
            ],
            "required": [
                "地方衛生研究所等",
                "民間検査機関等"
            ],
            "properties": {
                "地方衛生研究所等": {
                    "$id": "#/properties/data/properties/地方衛生研究所等",
                    "type": "array",
                    "title": "The 地方衛生研究所等 Schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": [],
                    "items": {
                        "$id": "#/properties/data/properties/地方衛生研究所等/items",
                        "type": "integer",
                        "title": "The Items Schema",
                        "description": "An explanation about the purpose of this instance.",
                        "default": 0,
                        "examples": [
                            0,
                            0
                        ]
                    }
                },
                "民間検査機関等": {
                    "$id": "#/properties/data/properties/民間検査機関等",
                    "type": "array",
                    "title": "The 民間検査機関等 Schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": [],
                    "items": {
                        "$id": "#/properties/data/properties/民間検査機関等/items",
                        "type": "integer",
                        "title": "The Items Schema",
                        "description": "An explanation about the purpose of this instance.",
                        "default": 0,
                        "examples": [
                            0,
                            0
                        ]
                    }
                }
            }
        },
        "labels": {
            "$id": "#/properties/labels",
            "type": "array",
            "title": "The Labels Schema",
            "description": "An explanation about the purpose of this instance.",
            "default": [],
            "items": {
                "$id": "#/properties/labels/items",
                "type": "string",
                "title": "The Items Schema",
                "description": "An explanation about the purpose of this instance.",
                "default": "",
                "pattern": "^([1-9]|1[0-2])/([1-9]|[12][0-9]|3[0-1])$",
                "examples": [
                    "1/1",
                    "1/2"
                ]
            }
        },
        "last_update": {
            "$id": "#/properties/last_update",
            "type": "string",
            "title": "The Last_update Schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}[+][0-9]{2}:[0-9]{2}$",
            "examples": [
                "2020-01-01T00:00:00+09:00"
            ]
        }
    }
}